{
  "name": "studenti-rs-api",
  "version": "1.0.0",
  "description": "API service for studenti.rs",
  "main": "index.ts",
  "scripts": {
    "pretty": "pino-pretty",
    "dev": "tsx watch --clear-screen=false src/index.ts | npm run pretty",
    "build": "rimraf build && tsc",
    "start": "NODE_ENV=production npm run build && tsx build/index.js | npm run pretty",
    "lint": "npx @biomejs/biome lint .",
    "lint:fix": "npx @biomejs/biome lint --apply .",
    "format:fix": "npx @biomejs/biome format' --write .",
    "format": "npx @biomejs/biome format .",
    "check:fix": "npx @biomejs/biome check . --apply",
    "test": "jest",
    "docker:build": "docker-compose build --no-cache",
    "docker:start": "docker-compose up",
    "docker:stop": "docker-compose down",
    "postinstall": "husky install"
  },
  "dependencies": {
    "bull": "^4.12.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "pino-http": "^9.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.2",
    "@tsconfig/node-lts-strictest-esm": "^18.12.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^6.0.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "pino-pretty": "^10.3.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*": [
      "biome check --no-errors-on-unmatched --files-ignore-unknown=true --apply"
    ]
  },
  "repository": "studenti-rs/api",
  "license": "UNLICENSED"
}
